                {
                        "label": "clean-ros",
                        "detail": "Clean ros colcon build",
                        "type": "shell",
                        "command": "colcon build --cmake-target clean",
                        "problemMatcher": "$gcc"
                },
                {
                        "label": "clean",
                        "detail": "Purge and clean ros colcon build",
