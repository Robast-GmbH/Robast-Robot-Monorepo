{
    "configurations": [
        {
            "browse": {
                "databaseFilename": "${default}",
                "limitSymbolsToIncludedHeaders": false
            },
            "includePath": [
                "/opt/$ROS_DISTRO/iron/include/**",
                "${workspaceFolder}/src/**",
                "${workspaceFolder}/install/**",
                "${workspaceFolder}/build/**",
                "${workspaceFolder}/libs/**"
            ],
            "name": "ROS",
            "intelliSenseMode": "gcc-x64",
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu11",
            "cppStandard": "c++17"
        }
    ],
    "version": 4
}