<?xml version="1.0" ?>
<sdf version='1.7'>
  <actor name="autonomous_actor">
    <pose>6.8 -10.5 0 0 0 0</pose>
    <skin>
      <filename>walk.dae</filename>
      <scale>1.0</scale>
    </skin>
    <animation name="walking">
      <filename>walk.dae</filename>
      <scale>1.000000</scale>
