        <model>cafe</model>
        <model>ground_plane</model>
      </ignore_obstacles>
      <targets>
        <target>6.8 -10.5 0</target>
        <target>11.5 -10.5 0</target>
        <target>11.5 -10.5 0</target>
        <target>6.8 -10.5 0</target>
      </targets>
    </plugin>
  </actor>
